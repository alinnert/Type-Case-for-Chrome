{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "A simple text editor for Chrome and Chrome OS",
   "file_handlers": {
      "any": {
         "extensions": [ "*" ]
      },
      "text": {
         "extensions": [ "md", "mdown", "markdown", "ini", "inf", "log", "js", "php" ],
         "types": [ "text/*" ]
      }
   },
   "icons": {
      "128": "assets/icon-128.png",
      "16": "assets/icon-16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "40",
   "name": "Type Case",
   "offline_enabled": true,
   "permissions": [ {
      "fileSystem": [ "write" ]
   }, "storage" ],
   "short_name": "Type Case",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.5.2"
}
